<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="property_type_action" model="ir.actions.act_window">
        <field name="name">Property type</field>
        <field name="res_model">property.type</field>
        <field name="view_mode">list,form</field>
    </record>




    <record id="property_type_view_form" model="ir.ui.view">
        <field name="name">property.type.view.form</field>
        <field name="model">property.type</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                    <notebook>
                        <page string="Properties">
                            <field name="type">
                                <list>
                                    <field name="name"/>
<!--                                    <field name="expected_price"/>-->
<!--                                    <field name="status"/>-->
                                </list>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>







    <record id="property_type_view_list" model="ir.ui.view">
        <field name="name">property.type.view.list</field>
        <field name="model">property.type</field>
        <field name="arch" type="xml">
            <list string="Property Types">
                <field name="sequence" widget="handle"/>
                <field name="name"/>
            </list>
        </field>
    </record>

</odoo>
